  {
    "name": "Pound to Kg Converter",
    "version": "1.0",
	"manifest_version": 2,
	"description": "Converts Pounds to Kilograms",
    "icons": {
		"16": "icon_16.png",
		"128": "icon_128.png"
    },
    "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },
    "permissions": ["activeTab", "contextMenus"],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }